                                </padding>
                                <HBox spacing="5.0">
                                    <Label text="%label.secondCrimeClassification" styleClass="bold-label"/>
                                    <Label fx:id="lblCrimeClassification2"/>
                                </HBox>
                                <TableView fx:id="tvCrimeClassification2">
                                    <columnResizePolicy>
                                        <TableView fx:constant="UNCONSTRAINED_RESIZE_POLICY"/>
                                    </columnResizePolicy>
                                    <placeholder>
                                        <Label text="%table.placeholder"/>
                                    </placeholder>
                                    <columns>
                                        <PTableColumn fx:id="tcInputSequence2" text="#" reorderable="false" sortable="false" percentageWidth="0.05"/>
                                        <PTableColumn fx:id="tcPartyName2" text="%table.column.partyName" reorderable="false" sortable="false" percentageWidth="0.3"/>
                                        <PTableColumn fx:id="tcSystemDecision2" text="%table.column.systemDecision" reorderable="false" sortable="false" percentageWidth="0.15"/>
                                        <PTableColumn fx:id="tcOperatorDecision2" text="%table.column.operatorDecision" reorderable="false" sortable="false" percentageWidth="0.25"/>
                                    </columns>
                                </TableView>
                            </VBox>
                        </TitledPane>
                        <TitledPane fx:id="tpCrimeClassification3" focusTraversable="false" collapsible="false" managed="false" visible="false">
                            <VBox spacing="10.0">
                                <padding>
                                    <Insets topRightBottomLeft="10.0"/>
                                </padding>
                                <HBox spacing="5.0">
                                    <Label text="%label.thirdCrimeClassification" styleClass="bold-label"/>
                                    <Label fx:id="lblCrimeClassification3"/>
                                </HBox>
                                <TableView fx:id="tvCrimeClassification3">
