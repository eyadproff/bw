                                    </columns>
                                </TableView>
                            </VBox>
                        </TitledPane>
                        <TitledPane fx:id="tpCrimeClassification5" focusTraversable="false" collapsible="false" managed="false" visible="false">
                            <VBox spacing="10.0">
                                <padding>
                                    <Insets topRightBottomLeft="10.0"/>
                                </padding>
                                <HBox spacing="5.0">
                                    <Label text="%label.fifthCrimeClassification" styleClass="bold-label"/>
                                    <Label fx:id="lblCrimeClassification5"/>
