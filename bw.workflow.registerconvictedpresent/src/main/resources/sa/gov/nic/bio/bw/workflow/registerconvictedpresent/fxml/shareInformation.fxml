<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import sa.gov.nic.bio.bw.workflow.commons.ui.PTableColumn?>
<BorderPane xmlns="http://javafx.com/javafx/11" xmlns:fx="http://javafx.com/fxml/1"
            fx:id="rootPane" stylesheets="@../css/style.css"
            fx:controller="sa.gov.nic.bio.bw.workflow.registerconvictedpresent.controllers.ShareInformationPaneFxController">
    <center>
        <BorderPane>
            <padding>
                <Insets top="10.0" bottom="10.0"/>
            </padding>
            <center>
                <ScrollPane fitToHeight="true" fitToWidth="true" hbarPolicy="NEVER">
                    <VBox alignment="CENTER" spacing="10.0">
                        <TitledPane fx:id="tpCrimeClassification1" focusTraversable="false" collapsible="false">
                            <VBox spacing="10.0">
                                <padding>
                                    <Insets topRightBottomLeft="10.0"/>
                                </padding>
                                <HBox spacing="5.0">
                                    <Label text="%label.firstCrimeClassification" styleClass="bold-label"/>
                                    <Label fx:id="lblCrimeClassification1"/>
                                </HBox>
                                <TableView fx:id="tvCrimeClassification1">
                                    <columnResizePolicy>
                                        <TableView fx:constant="UNCONSTRAINED_RESIZE_POLICY"/>
                                    </columnResizePolicy>
                                    <placeholder>
                                        <Label text="%table.placeholder"/>
                                    </placeholder>
                                    <columns>
                                        <PTableColumn fx:id="tcInputSequence1" text="#" reorderable="false" sortable="false" percentageWidth="0.05"/>
                                        <PTableColumn fx:id="tcPartyName1" text="%table.column.partyName" reorderable="false" sortable="false" percentageWidth="0.3"/>
                                        <PTableColumn fx:id="tcSystemDecision1" text="%table.column.systemDecision" reorderable="false" sortable="false" percentageWidth="0.15"/>
                                        <PTableColumn fx:id="tcOperatorDecision1" text="%table.column.operatorDecision" reorderable="false" sortable="false" percentageWidth="0.25"/>
                                    </columns>
                                </TableView>
                            </VBox>
                        </TitledPane>
                        <TitledPane fx:id="tpCrimeClassification2" focusTraversable="false" collapsible="false" managed="false" visible="false">
