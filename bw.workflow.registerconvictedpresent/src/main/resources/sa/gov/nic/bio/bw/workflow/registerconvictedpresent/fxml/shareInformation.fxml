                                        <PTableColumn fx:id="tcOperatorDecision3" text="%table.column.operatorDecision" reorderable="false" sortable="false" percentageWidth="0.25"/>
                                    </columns>
                                </TableView>
                            </VBox>
                        </TitledPane>
                        <TitledPane fx:id="tpCrimeClassification4" focusTraversable="false" collapsible="false" managed="false" visible="false">
                            <VBox spacing="10.0">
                                <padding>
