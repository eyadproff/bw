<BorderPane xmlns="http://javafx.com/javafx/11" xmlns:fx="http://javafx.com/fxml/1"
            fx:id="rootPane" fx:controller="sa.gov.nic.bio.bw.workflow.registerconvictedpresent.controllers.PalmCapturingFxController"
            stylesheets="@../css/style.css">
    <center>
        <VBox>
            <padding>
                <Insets top="10" bottom="10"/>
            </padding>
            <SplitPane orientation="VERTICAL" dividerPositions="0.5" VBox.vgrow="ALWAYS">
                <SplitPane styleClass="white-background" dividerPositions="0.6,0.8">
                    <TitledPane text="%label.fingerprintDeviceControlPanel" maxHeight="Infinity" collapsible="false" focusTraversable="false">
                        <ScrollPane fx:id="paneControlsOuterContainer" fitToWidth="true" VBox.vgrow="ALWAYS" maxHeight="Infinity">
                            <VBox fx:id="paneControlsInnerContainer" alignment="CENTER" spacing="10.0">
                                <padding>
                                    <Insets topRightBottomLeft="10.0"/>
                                </padding>
                                <ProgressIndicator fx:id="piProgress" prefHeight="25.0" prefWidth="25.0" managed="false" visible="false"/>
                                <ImageView fx:id="ivCompleted" managed="false" visible="false">
                                    <Image url="@/sa/gov/nic/bio/bw/commons/resources/images/icon_success_16px.png"/>
                                </ImageView>
                                <Label fx:id="lblStatus" wrapText="true" textAlignment="CENTER" managed="false" visible="false"/>
                                <VBox alignment="CENTER" spacing="5.0">
                                    <Button fx:id="btnCancel" text="%button.cancel" managed="false" visible="false" wrapText="true" textAlignment="CENTER"/>
                                    <Button fx:id="btnStartFingerprintCapturing" maxHeight="Infinity" text="%button.captureRightLowerPalmFingerprint" managed="false" visible="false" wrapText="true" textAlignment="CENTER" onAction="#onStartFingerprintCapturingButtonClicked"/>
                                    <Button fx:id="btnStopFingerprintCapturing" text="%button.stopFingerprintCapturing" managed="false" visible="false" wrapText="true" textAlignment="CENTER" onAction="#onStopFingerprintCapturingButtonClicked"/>
                                    <Button fx:id="btnStartOverFingerprintCapturing" text="%button.startOver" managed="false" visible="false" wrapText="true" textAlignment="CENTER" onAction="#onStartOverFingerprintCapturingButtonClicked"/>
                                </VBox>
                            </VBox>
                        </ScrollPane>
                    </TitledPane>
                    <FourStateTitledPane fx:id="tpFingerprintDeviceLivePreview" text="%label.fingerprintDeviceLivePreview" maxHeight="Infinity" collapsible="false" focusTraversable="false">
                        <graphic>
                            <Button fx:id="btnLivePreviewLegend" focusTraversable="false">
                                <padding>
                                    <Insets top="1.0" bottom="3.0" right="5.0" left="5.0"/>
                                </padding>
                            </Button>
                        </graphic>
                        <StackPane>
                            <ImageViewPane>
                                <imageView>
                                    <ImageView fx:id="ivFingerprintDeviceLivePreviewPlaceholder" preserveRatio="true">
                                        <Image url="@/sa/gov/nic/bio/bw/commons/resources/images/placeholder_image.png"/>
                                    </ImageView>
                                </imageView>
                            </ImageViewPane>
                            <ImageViewPane minHeight="79.0">
                                <imageView>
                                    <ImageView fx:id="ivFingerprintDeviceLivePreview" preserveRatio="true"/>
                                </imageView>
                            </ImageViewPane>
                            <ProgressIndicator fx:id="piFingerprintDeviceLivePreview" maxHeight="25.0" maxWidth="25.0" managed="false" visible="false"/>
                        </StackPane>
                    </FourStateTitledPane>
