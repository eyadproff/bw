<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<AnchorPane xmlns="http://javafx.com/javafx/8.0.152" xmlns:fx="http://javafx.com/fxml/1"
            fx:id="rootPane" fx:controller="sa.gov.nic.bio.bw.core.controllers.FooterPaneFxController">
    <HBox alignment="CENTER" spacing="7.0" AnchorPane.topAnchor="0.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
        <ImageView>
            <Image url="@../images/semat_logo_small.png"/>
        </ImageView>
        <Label text="%label.developedBy" styleClass="credit-text"/>
    </HBox>
    <CheckBox fx:id="chbMockTasks" text="%checkbox.mockTasks" AnchorPane.leftAnchor="0.0" AnchorPane.bottomAnchor="0.0" focusTraversable="false" visible="false" managed="false"/>
</AnchorPane>