                                            </imageView>
                                        </ImageViewPane>
                                    </StackPane>
                                </TitledPane>
                                <TitledPane text="%label.fingers.ring" collapsible="false" wrapText="true" maxHeight="Infinity" graphicTextGap="0.0" HBox.hgrow="ALWAYS" VBox.vgrow="ALWAYS" GridPane.rowIndex="0" GridPane.columnIndex="3" GridPane.vgrow="ALWAYS" focusTraversable="false">
                                    <StackPane>
                                        <ImageViewPane minHeight="35.0">
