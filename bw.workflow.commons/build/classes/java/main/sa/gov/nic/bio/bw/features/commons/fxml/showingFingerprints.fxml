            fx:id="rootPane" stylesheets="@../css/style.css">
    <center>
        <VBox alignment="CENTER">
            <HBox alignment="CENTER" VBox.vgrow="ALWAYS">
                <TitledPane text="%label.fingerprints" collapsible="false" focusTraversable="false" maxHeight="Infinity" HBox.hgrow="ALWAYS" VBox.vgrow="ALWAYS">
                    <padding>
                        <Insets top="5.0" bottom="5.0"/>
                    </padding>
                    <VBox alignment="CENTER" spacing="5.0">
                        <TitledPane text="%label.rightHand" collapsible="false" wrapText="true" maxHeight="Infinity" focusTraversable="false" VBox.vgrow="ALWAYS">
                            <GridPane vgap="5" maxHeight="Infinity" nodeOrientation="LEFT_TO_RIGHT">
                                <padding>
                                    <Insets topRightBottomLeft="5.0"/>
                                </padding>
                                <columnConstraints>
                                    <ColumnConstraints percentWidth="20"/>
                                    <ColumnConstraints percentWidth="20"/>
                                    <ColumnConstraints percentWidth="20"/>
                                    <ColumnConstraints percentWidth="20"/>
                                    <ColumnConstraints percentWidth="20"/>
                                </columnConstraints>
                                <TitledPane text="%label.fingers.thumb" collapsible="false" wrapText="true" maxHeight="Infinity" graphicTextGap="0.0" HBox.hgrow="ALWAYS" VBox.vgrow="ALWAYS" GridPane.rowIndex="0" GridPane.columnIndex="0" GridPane.vgrow="ALWAYS" focusTraversable="false">
                                    <StackPane>
                                        <ImageViewPane minHeight="35.0">
                                            <imageView>
                                                <ImageView fx:id="ivRightThumb" preserveRatio="true">
                                                    <Image url="@../images/skip_icon.png"/>
                                                </ImageView>
                                            </imageView>
                                        </ImageViewPane>
                                    </StackPane>
                                </TitledPane>
                                <TitledPane text="%label.fingers.index" collapsible="false" wrapText="true" maxHeight="Infinity" graphicTextGap="0.0" HBox.hgrow="ALWAYS" VBox.vgrow="ALWAYS" GridPane.rowIndex="0" GridPane.columnIndex="1" GridPane.vgrow="ALWAYS" focusTraversable="false">
