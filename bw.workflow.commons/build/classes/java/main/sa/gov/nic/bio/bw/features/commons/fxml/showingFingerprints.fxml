                                        <ImageViewPane minHeight="35.0">
                                            <imageView>
                                                <ImageView fx:id="ivRightMiddle" preserveRatio="true">
                                                    <Image url="@../images/skip_icon.png"/>
                                                </ImageView>
                                            </imageView>
                                        </ImageViewPane>
                                    </StackPane>
                                </TitledPane>
                                <TitledPane text="%label.fingers.ring" collapsible="false" wrapText="true" maxHeight="Infinity" graphicTextGap="0.0" HBox.hgrow="ALWAYS" VBox.vgrow="ALWAYS" GridPane.rowIndex="0" GridPane.columnIndex="3" GridPane.vgrow="ALWAYS" focusTraversable="false">
                                    <StackPane>
                                        <ImageViewPane minHeight="35.0">
                                            <imageView>
                                                <ImageView fx:id="ivRightRing" preserveRatio="true">
                                                    <Image url="@../images/skip_icon.png"/>
                                                </ImageView>
                                            </imageView>
                                        </ImageViewPane>
                                    </StackPane>
                                </TitledPane>
                                <TitledPane text="%label.fingers.little" collapsible="false" wrapText="true" maxHeight="Infinity" graphicTextGap="0.0" HBox.hgrow="ALWAYS" VBox.vgrow="ALWAYS" GridPane.rowIndex="0" GridPane.columnIndex="4" GridPane.vgrow="ALWAYS" focusTraversable="false">
                                    <StackPane>
                                        <ImageViewPane minHeight="35.0">
                                            <imageView>
                                                <ImageView fx:id="ivRightLittle" preserveRatio="true">
                                                    <Image url="@../images/skip_icon.png"/>
                                                </ImageView>
                                            </imageView>
                                        </ImageViewPane>
                                    </StackPane>
                                </TitledPane>
                            </GridPane>
                        </TitledPane>
                        <TitledPane text="%label.leftHand" collapsible="false" wrapText="true" maxHeight="Infinity" focusTraversable="false" VBox.vgrow="ALWAYS">
                            <GridPane vgap="5" maxHeight="Infinity" nodeOrientation="LEFT_TO_RIGHT">
                                <padding>
                                    <Insets topRightBottomLeft="5.0"/>
                                </padding>
                                <columnConstraints>
                                    <ColumnConstraints percentWidth="20"/>
