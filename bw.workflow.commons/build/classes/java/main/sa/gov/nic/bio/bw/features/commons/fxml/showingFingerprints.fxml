                                    <StackPane>
                                        <ImageViewPane minHeight="35.0">
                                            <imageView>
                                                <ImageView fx:id="ivLeftLittle" preserveRatio="true">
                                                    <Image url="@../images/skip_icon.png"/>
                                                </ImageView>
                                            </imageView>
                                        </ImageViewPane>
                                    </StackPane>
                                </TitledPane>
                            </GridPane>
                        </TitledPane>
                    </VBox>
                </TitledPane>
            </HBox>
        </VBox>
    </center>
    <bottom>
        <HBox alignment="CENTER" spacing="10">
            <padding>
                <Insets bottom="10"/>
            </padding>
            <Button fx:id="btnPrevious" text="%button.previous" onAction="#onPreviousButtonClicked"/>
            <Button fx:id="btnInquiry" text="%button.inquiry" onAction="#onInquiryButtonClicked"/>
        </HBox>
    </bottom>
</BorderPane>