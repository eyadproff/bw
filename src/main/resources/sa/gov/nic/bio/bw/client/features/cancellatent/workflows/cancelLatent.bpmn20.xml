<?xml version="1.0" encoding="UTF-8"?>
<definitions id="definitions"
             targetNamespace="http://activiti.org/bpmn20"
             xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL"
             xmlns:activiti="http://activiti.org/bpmn">

    <process id="menu.cancel.cancelLatent" name="Cancel Latent Process">

        <startEvent id="startEvent"/>

        <sequenceFlow sourceRef="startEvent" targetRef="dataEntry"/>

        <userTask id="dataEntry" activiti:formKey="sa.gov.nic.bio.bw.client.features.cancellatent.CancelLatentPaneFxController">
            <extensionElements>
                <activiti:formProperty id="personId" type="string" required="true"/>
                <activiti:formProperty id="latentId" type="string" required="true"/>
            </extensionElements>
        </userTask>

        <sequenceFlow sourceRef="dataEntry" targetRef="cancelLatentService"/>

        <serviceTask id="cancelLatentService" name="Calling CancelLatent API" activiti:class="sa.gov.nic.bio.bw.client.features.cancellatent.workflow.CancelLatentService"/>

        <sequenceFlow sourceRef="cancelLatentService" targetRef="dataEntry"/>

        <endEvent id="endEvent" />

    </process>

</definitions>