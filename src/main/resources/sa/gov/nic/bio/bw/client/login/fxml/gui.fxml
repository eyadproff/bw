<?import sa.gov.nic.bio.bw.client.features.commons.ui.AutoScalingStackPane?>
<VBox xmlns="http://javafx.com/javafx/8.0.152" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="sa.gov.nic.bio.bw.client.login.LoginPaneFxController"
      fx:id="rootPane" stylesheets="@../css/style.css" alignment="CENTER" spacing="15.0">
    <padding>
        <Insets topRightBottomLeft="10.0"/>
    </padding>
    <ImageView>
        <Image url="@../../core/images/nic_logo_150.png"/>
    </ImageView>
    <VBox alignment="CENTER" spacing="10.0">
        <HBox alignment="CENTER">
            <TabPane fx:id="tabPane" styleClass="floating">
                <Tab fx:id="tabLoginByPassword" text="%tab.loginByPassword" closable="false">
                    <GridPane alignment="CENTER" vgap="5.0" hgap="20.0" styleClass="formBox-with-tabs">
                        <padding>
                            <Insets top="20.0" right="20.0" bottom="20.0" left="20.0"/>
                        </padding>
                        <Label text="%label.username" GridPane.rowIndex="0" GridPane.columnIndex="0"/>
                        <TextField fx:id="txtUsernameLoginByPassword" GridPane.rowIndex="0" GridPane.columnIndex="1"/>
                        <Label text="%label.password" GridPane.rowIndex="1" GridPane.columnIndex="0"/>
                        <PasswordField fx:id="txtPassword" GridPane.rowIndex="1" GridPane.columnIndex="1"/>
                        <Label text="%label.language" GridPane.rowIndex="2" GridPane.columnIndex="0"/>
                        <VBox GridPane.rowIndex="2" GridPane.columnIndex="1">
                            <ComboBox visible="false" managed="false"/> <!-- workaround to remove the additional arrow -->
                            <ComboBox fx:id="cboLanguage"/>
                        </VBox>
                    </GridPane>
                </Tab>
                <Tab fx:id="tabLoginByFingerprint" text="%tab.loginByFingerprint" closable="false">
                    <GridPane alignment="CENTER" vgap="5.0" hgap="20.0" styleClass="formBox-with-tabs">
                        <padding>
                            <Insets top="20.0" right="20.0" bottom="20.0" left="20.0"/>
                        </padding>
                        <Label text="%label.username" GridPane.rowIndex="0" GridPane.columnIndex="0"/>
                        <TextField fx:id="txtUsernameLoginByFingerprint" GridPane.rowIndex="0" GridPane.columnIndex="1"/>
                        <Label text="%label.fingerprintScanner" GridPane.rowIndex="1" GridPane.columnIndex="0"/>
                        <HBox alignment="CENTER_LEFT" spacing="10.0" GridPane.rowIndex="1" GridPane.columnIndex="1">
                            <Label fx:id="lblFingerprintScannerNotInitialized" text="%label.notInitialized">
                                <graphic>
                                    <ImageView>
                                        <Image url="@../../core/images/yellow_status.png"/>
                                    </ImageView>
                                </graphic>
                            </Label>
                            <Label fx:id="lblFingerprintScannerNotConnected" text="%label.notConnected" visible="false" managed="false">
                                <graphic>
                                    <ImageView>
                                        <Image url="@../../core/images/red_status.png"/>
                                    </ImageView>
                                </graphic>
                            </Label>
                            <Label fx:id="lblFingerprintScannerInitialized" text="%label.initialized" visible="false" managed="false">
                                <graphic>
                                    <ImageView>
                                        <Image url="@../../core/images/green_status.png"/>
                                    </ImageView>
                                </graphic>
                            </Label>
                            <Button fx:id="btnFingerprintScannerAction" onMouseClicked="#onFingerprintScannerActionButtonClicked"/>
                        </HBox>
                        <VBox GridPane.rowIndex="2" GridPane.columnIndex="0">
                            <padding>
                                <Insets top="3.0"/>
                            </padding>
                            <Label text="%label.fingerprint" alignment="TOP_LEFT"/>
                        </VBox>
                        <HBox alignment="CENTER_LEFT" spacing="5.0" GridPane.rowIndex="2" GridPane.columnIndex="1">
                            <VBox>
                                <Button fx:id="btnChangeFingerprint" text="%button.change" alignment="TOP_LEFT" onAction="#onChangeFingerprintButtonClicked"/>
                            </VBox>
                            <HBox alignment="CENTER" HBox.hgrow="ALWAYS">
                                <HBox alignment="CENTER" nodeOrientation="LEFT_TO_RIGHT" spacing="5.0">
