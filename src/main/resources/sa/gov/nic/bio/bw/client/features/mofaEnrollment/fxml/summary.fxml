<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.geometry.Insets?>

<BorderPane xmlns="http://javafx.com/javafx/8.0.152" xmlns:fx="http://javafx.com/fxml/1"
            fx:id="rootPane" fx:controller="sa.gov.nic.bio.bw.client.features.mofaenrollment.SummaryFxController">
    <center>
        <Label text="Summary"/>
    </center>
    <bottom>
        <HBox alignment="CENTER" spacing="10">
            <padding>
                <Insets bottom="10"/>
            </padding>
            <Button fx:id="btnPrevious" text="%button.previous"/>
            <Button fx:id="btnSubmit" text="%button.submit"/>
        </HBox>
    </bottom>
</BorderPane>